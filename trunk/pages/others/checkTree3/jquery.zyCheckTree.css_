UL.tree li{list-style-type: none; border:0px;}

UL.tree LI .arrow
{
    width: 16px;
    height: 16px;
    padding: 0;
    margin: 0;
    cursor: default;
    float: left;
    background: transparent no-repeat 0 0px;
}

UL.tree li .collapsed {
    cursor: pointer;
    background-image: url(collapsed.gif);
}

UL.tree li .expanded {
    cursor: pointer;
    background-image: url(expanded.gif);
}

UL.tree LI .checkbox {
 PADDING-BOTTOM: 0px; 
 MARGIN: 0px; 
 PADDING-LEFT: 0px; 
 WIDTH: 20px; 
 PADDING-RIGHT: 
 0px; BACKGROUND: url(check0.gif) no-repeat 0px 0px; FLOAT: left;
  HEIGHT: 18px; 
 cursor: pointer; 
 PADDING-TOP: 0px
}
UL.tree LI .checked {
    BACKGROUND-IMAGE: url(check2.gif)
}
UL.tree LI .half_checked {
    BACKGROUND-IMAGE: url(check1.gif)
}

UL.tree LI.loseControl
{
    border: solid 1px red;
} 

UL.tree LI.loseControl .checkbox
{
  cursor: default;
} 

UL.tree label
{
  cursor: pointer;
} 


UL.tree LI.loseControl label
{
  cursor: default;
} 
